{
    "files.associations": {
        "termprint.h": "c",
        "uarmmake": "makefile",
        "umpsmake": "makefile",
        "system.h": "c",
        "libumps.h": "c",
        "**/uarm/**/*": "plaintext",
        "**/umps/**/*": "plaintext"
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0 }",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}",
        "uarm",
        "umps",
        "include"
    ],
    "C_Cpp.default.defines": [
        "TARGET_UMPS",
        "TARGET_UARM"
    ]
}