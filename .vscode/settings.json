{
    "files.associations": {
        "**/uarm/**/*": "plaintext",
        "**/umps/**/*": "plaintext",
        "term.h": "c",
        "cstdint": "cpp"
    },
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 4, ColumnLimit: 0 }",
    "C_Cpp.default.includePath": [
        "${workspaceFolder}",
        "uarm",
        "umps",
        "include"
    ],
    "C_Cpp.default.defines": [
        "TARGET_UMPS=1",
        "TARGET_UARM=1"
    ]
}